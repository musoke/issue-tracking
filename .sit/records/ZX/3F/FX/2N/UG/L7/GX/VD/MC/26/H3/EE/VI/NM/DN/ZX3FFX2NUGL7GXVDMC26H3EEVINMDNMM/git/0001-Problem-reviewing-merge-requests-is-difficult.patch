+                    }
+                }
+            }
+
+            filterMergeFiles(files) {
+                return files.filter(function(f) {
+                   return f.startsWith("git/") || f.startsWith("patch/")
+                });
+            }
+
+        }
+        customElements.define(MergeRequest.is, MergeRequest);
+    </script>
+</dom-module>
